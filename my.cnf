[mysqld]
character-set-server=utf8mb4
collation-server=utf8mb4_general_ci
explicit-defaults-for-timestamp=1
general-log=1
general-log-file=/var/log/mysql/mysqld.log

[client]
default-character-set=utf8mb4

[mysql]
default-character-set=utf8mb4

[mysqladmin]
default-character-set=utf8mb4

[mysqldump]
default-character-set=utf8mb4

[mysqlimport]
default-character-set=utf8mb4

[mysqlshow]
default-character-set=utf8mb4

[myisamchk]
default-character-set=utf8mb4

[myisampack]
default-character-set=utf8mb4